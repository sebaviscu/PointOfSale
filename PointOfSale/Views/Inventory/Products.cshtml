@{
    ViewData["Title"] = "Products";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@section StylesPOS {

    <link href="~/plugins/data-tables/datatables.bootstrap4.min.css" rel="stylesheet">
    <link href="~/plugins/data-tables/buttons.bootstrap4.min.css" rel="stylesheet">
    <link href="~/plugins/data-tables/responsive.datatables.min.css" rel="stylesheet">
    <link href="~/plugins/jquery-ui/jquery-ui.css" rel="stylesheet">

}


<style>
    #tablaContainer td:nth-last-child(-n+5) {
        border: 2.3px dotted lightslategray;
    }


    #tablaContainer > table {
        border-collapse: collapse;
        width: 100%;
    }

        #tablaContainer > table th,
        #tablaContainer > table td {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }

        #tablaContainer > table tr:nth-child(even) {
            background-color: #f2f2f2;
        }

        #tablaContainer > table td.editable {
            cursor: pointer;
        }

            #tablaContainer > table td.editable:focus {
                outline: 1px solid blue;
            }
</style>

<div class="row">
    <div class="col-sm-12">

        <div class="card card-default">
            <div class="card-header card-header-border-bottom">
                <div>
                    <h4>Productos</h4>
                </div>
                <div class="ms-auto bd-highlight">
                    <button class="btn btn-primary btn-sm" id="btnEditAll" onclick="editAll()">Editar Seleccion</button>

                    <button class="btn btn-info btn-sm ms-4 " id="btnEditAll" onclick="imprimirPrecios()">Imprimir Precio</button>

                    <button class="btn btn-secondary btn-sm ms-4 " id="bntModalImportar">Importar</button>

                    <button class="btn btn-success btn-sm ms-4 " id="btnNewProduct">Nuevo Producto</button>

                </div>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-sm-12">
                        <table id="tbData" class="table table-striped" style="width:100%">
                            <thead>
                                <tr>
                                    <th></th>
                                    <th>
                                        <input type="checkbox" id="chkSelectAll">
                                    </th>
                                    @* <th>Foto</th> *@
                                    <th>Descripción</th>
                                    <th>Categoria</th>
                                    <th>Proveedor</th>
                                    <th>Precio (1)</th>
                                    <th>Fecha Modific.</th>
                                    <th>Estado</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>


